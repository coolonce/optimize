        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kursOptimiz"
        mc:Ignorable="d"
        Title="Выбор задачи" Height="174.079" Width="261.184" Initialized="windowSelectTask_Initialized" Closed="windowSelectTask_Closed">
    <Grid>
        <ComboBox x:Name="selectTaskcomboBox" HorizontalAlignment="Center" Margin="49,60,39,0" VerticalAlignment="Top" Width="165" SelectionChanged="comboBox_SelectionChanged"/>
